
<html>
	<head>
		<title>@Model.Title</title>

      	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>

		<link rel="stylesheet" media="screen,projection" href="materialize/css/materialize.min.css">
	</head>
	<body>
		<div class="navbar-fixed">
			<nav role="navigation" class="blue-grey darken-3">
				<div class="nav-wrapper container">
					<a id="logo-container" href="/" class="brand-logo">service manager</a>

					<ul class="right hide-on-med-and-down">
						<li><a href="/"><i class="mdi-navigation-refresh"></i></a></li>
					</ul>
					<div id="nav-mobile" class="side-nav">
						<ul>
							<li><a href="/">Refresh</a></li>
						</ul>
					</div>

					<a href="#!" data-activates="nav-mobile" class="button-collapse">
						<i class="mdi-navigation-menu"></i>
					</a>
				</div>
			</nav>
		</div>

		@Section['Content'];

      	<script src="jquery/jquery-2.1.4.min.js"></script>
		<script src="materialize/js/materialize.min.js"></script>
		<script src="app.js"></script>

		@Section['Scripts'];
	</body>
</html>
